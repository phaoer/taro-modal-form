{
	"compilerOptions": {
		"declaration": false,
		"esModuleInterop": true,
		"jsx": "react",
		"lib": ["es6", "dom"],
		"target": "es6",
		"moduleResolution": "node",
		"noImplicitAny": false,
		"resolveJsonModule": true,
		"skipLibCheck": true
	},
	"include": ["src"],
	"exclude": ["node_modules", "dist"]
}
